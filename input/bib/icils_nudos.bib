@article{wout_many_2008,
  title = {The Many Faces of Stereotype Threat: {{Group-}} and Self-Threat},
  shorttitle = {The Many Faces of Stereotype Threat},
  author = {Wout, Daryl and Danso, Henry and Jackson, James and Spencer, Steve},
  year = {2008},
  month = may,
  journal = {Journal of Experimental Social Psychology},
  volume = {44},
  number = {3},
  pages = {792--799},
  issn = {0022-1031},
  doi = {10.1016/j.jesp.2007.07.005},
  urldate = {2023-12-12},
  abstract = {Contending with negative intellectual stereotypes has been shown to depress the academic performance of targets of the stereotypes [Steele, C. M. (1997). A threat in the air: How stereotypes shape intellectual identity and performance. American Psychologist, 52, 613--629]. The present paper examines whether women's mathematics performance is differentially affected by the concern of confirming that a negative stereotype is true of the self (self-threat), than by the concern of confirming that the stereotype is true of their gender (group-threat). In two studies we independently manipulated these different threats for women taking a mathematics test. Gender identification moderated the effect of group-threats on test performance; only women highly identified with their gender underperformed. The performance of less gender-identified women was unaffected by group-threats. In contrast, gender identification did not moderate the effect of self-threats---both high- and low-identified women underperformed. The results of these studies suggest that women's math performance is differentially affected by the source of the threat.},
  keywords = {Gender,Self and identity,Stereotype threat,Stereotyping,Stigma}
}

