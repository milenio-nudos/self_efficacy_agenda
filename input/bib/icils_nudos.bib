@article{brief_self_1981,
  title = {The ``{{Self}}'' in {{Work Organizations}}: {{A Conceptual Review}}},
  shorttitle = {The ``{{Self}}'' in {{Work Organizations}}},
  author = {Brief, Arthur P. and Aldag, Ramon J.},
  year = {1981},
