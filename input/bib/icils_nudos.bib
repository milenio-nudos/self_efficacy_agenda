@article{schwarzer_socialcognitive_2000,
  title = {Social-Cognitive Predictors of Health Behavior: {{Action}} Self-Efficacy and Coping Self-Efficacy},
  shorttitle = {Social-Cognitive Predictors of Health Behavior},
  author = {Schwarzer, Ralf and Renner, Britta},
  year = {2000},
