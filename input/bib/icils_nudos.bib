  author = {Claro, Magdalena and Preiss, David D. and San Mart{\'i}n, Ernesto and Jara, Ignacio and Hinostroza, J. Enrique and Valenzuela, Susana and Cortes, Flavio and Nussbaum, Miguel},
  year = {2012},
