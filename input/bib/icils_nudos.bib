@article{davis_heuristic_2013,
  title = {A Heuristic--Systematic Model of End-User Information Processing When Encountering {{IS}} Exceptions},
  author = {Davis, Joshua M. and Tuttle, Brad M.},
  year = {2013},
  month = mar,
